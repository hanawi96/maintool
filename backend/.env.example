# MP3 Cutter Pro Environment Variables

# Server Configuration
PORT=3001
NODE_ENV=development
SESSION_SECRET=your-super-secret-session-key-change-in-production
ENCRYPTION_KEY=your-32-byte-encryption-key-for-token-storage

# Cloud Storage Mock Mode (for testing without OAuth setup)
CLOUD_MOCK_MODE=true

# Google Drive OAuth
GOOGLE_DRIVE_CLIENT_ID=your-google-client-id
GOOGLE_DRIVE_CLIENT_SECRET=your-google-client-secret
GOOGLE_DRIVE_REDIRECT_URI=http://localhost:3001/api/cloud/callback/google_drive

# Dropbox OAuth  
DROPBOX_CLIENT_ID=your-dropbox-app-key
DROPBOX_CLIENT_SECRET=your-dropbox-app-secret
DROPBOX_REDIRECT_URI=http://localhost:3001/api/cloud/callback/dropbox

# OneDrive OAuth
ONEDRIVE_CLIENT_ID=your-onedrive-client-id
ONEDRIVE_CLIENT_SECRET=your-onedrive-client-secret
ONEDRIVE_REDIRECT_URI=http://localhost:3001/api/cloud/callback/onedrive

